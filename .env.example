# Liberation Analytics Environment Configuration

# Database Configuration
POSTGRES_PASSWORD=your_secure_postgres_password_here

# Analytics Configuration  
ENVIRONMENT=production
DUCKDB_PATH=/app/data/analytics.db

# Security
API_TOKEN_SECRET=your_random_256_bit_secret_for_jwt_signing

# Optional: GeoIP (download GeoLite2-Country.mmdb)
GEOIP_ENABLED=true
GEOIP_DATABASE_PATH=/app/data/GeoLite2-Country.mmdb

# Optional: Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_MINUTE=100

# Optional: CORS Origins (comma-separated)
CORS_ORIGINS=https://greenfieldoverride.com,https://staging.greenfieldoverride.com